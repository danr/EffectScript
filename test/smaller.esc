
effect iter<a> {
  yield(a)
}

function sum(i: r ! iter<int> ! e): int ! e {
  switch next(i) {
    case yield(x, k):
          puts("A: " + show(x))
          sum({k("first")});
          puts("B: " + show(x))
          sum({k("second")});
          puts("C: " + show(x))
    case done(r): puts("done") 1
  }
}

function onetwo() {
    u1 = yield(1)
    u2 = yield(2)
    puts("end")
    2
}

function main() {
  sum(onetwo)
}


